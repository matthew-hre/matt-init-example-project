{
  "name": "matt-init-example-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm run dev:db\" \"next dev --turbopack\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev:db": "docker-compose up db",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down && pnpm run dev:db",
    "db:wipe": "docker-compose down -v && pnpm run dev:db && pnpm run db:push"
  },
  "dependencies": {
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.30",
    "drizzle-orm": "latest",
    "better-auth": "latest",
    "pg": "latest",
    "postgres": "latest",
    "uuid": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-plugin-better-tailwindcss": "^3.4.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tailwindcss": "^4",
    "typescript": "^5",
    "drizzle-kit": "latest",
    "concurrently": "latest",
    "@types/pg": "latest"
  }
}
