{
  "name": "matt-init-example-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm run dev:db\" \"next dev --turbopack\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev:db": "docker-compose up db",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down && pnpm run dev:db",
    "db:wipe": "docker-compose down -v && pnpm run dev:db && pnpm run db:push",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@uploadthing/react": "^7.3.2",
    "better-auth": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "latest",
    "lucide-react": "^0.525.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "pg": "latest",
    "postgres": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.3",
    "uuid": "latest",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "latest",
    "drizzle-kit": "latest",
    "eslint": "^9",
    "eslint-plugin-better-tailwindcss": "^3.4.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  }
